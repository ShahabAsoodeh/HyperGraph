author = {Boissard, Emmanuel and {Le Gouic}, Thibaut},
doi = {10.1214/12-AIHP517},
file = {:Users/trgao10/Library/Application Support/Mendeley Desktop/Downloaded/Boissard, Le Gouic - 2014 - On the Mean Speed of Convergence of Empirical and Occupation Measures in Wasserstein Distance.pdf:pdf},
journal = {Annales de l'Institut Henri Poincar{\'{e}} - Probabilit{\'{e}}s et Statistiques},
number = {2},
pages = {539--563},
title = {{On the Mean Speed of Convergence of Empirical and Occupation Measures in Wasserstein Distance}},
url = {www.imstat.org/aihpAnnalesdel'},
volume = {50},
year = {2014}
}


