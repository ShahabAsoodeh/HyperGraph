@article{DSS2013,
abstract = {In this article, we study the approximation of a probability measure $\mu$ on R d by its empirical measur{\^{e}} $\mu$ N interpreted as a random quantization. As error criterion we consider an averaged pth moment Wasserstein metric. In the case where 2p {\textless} d, we establish fine upper and lower bounds for the error, a high resolution formula. Moreover, we provide a universal estimate based on moments, a Pierce type estimate. In particular, we show that quantization by empirical measures is of optimal order under weak assumptions. R{\'{e}}sum{\'{e}}. Dans cet article, nous {\'{e}}tudions l'approximation d'une mesure de probabilit{\'{e}} $\mu$ sur R d par sa mesure empiriqu{\^{e}} $\mu$ N , interpr{\'{e}}t{\'{e}}e comme quantification al{\'{e}}atoire. Comme crit{\`{e}}re d'erreur, nous consid{\'{e}}rons une moyenne de m{\'{e}}trique de Wasserstein d'ordre p. Dans le cas 2p {\textless} d, nous {\'{e}}tablissons des bornes sup{\'{e}}rieures et inf{\'{e}}rieures am{\'{e}}lior{\'{e}}es pour l'erreur, une formule haute r{\'{e}}-solution. De plus, nous donnons une estimation universelle {\`{a}} base de moments, nomm{\'{e}}ee estimation du type Pierce. En particulier, nous prouvons que, sous de faibles hypoth{\`{e}}ses, la quantification par des mesures empiriques est d'ordre optimal. MSC: Primary 60F25; secondary 65D32},
author = {Dereich, Steffen and Scheutzow, Michael and Schottstedt, Reik},
doi = {10.1214/12-AIHP489},
file = {:Users/trgao10/Library/Application Support/Mendeley Desktop/Downloaded/Dereich, Scheutzow, Schottstedt - 2013 - Constructive Quantization Approximation by Empirical Measures.pdf:pdf},
journal = {Annales de l'Institut Henri Poincar{\'{e}} - Probabilit{\'{e}}s et Statistiques},
number = {4},
pages = {1183--1203},
title = {{Constructive Quantization: Approximation by Empirical Measures}},
url = {https://projecteuclid.org/download/pdfview{\_}1/euclid.aihp/1380718743},
volume = {49},
year = {2013}
}
@article{Lei2018,
abstract = {We provide upper bounds of the expected Wasserstein distance between a probability measure and its empirical version, generalizing recent results for finite dimensional Euclidean spaces and bounded functional spaces. Such a generalization can cover Euclidean spaces with large dimensionality, with the optimal dependence on the dimensionality. Our method also covers the important case of Gaussian processes in separable Hilbert spaces, with rate-optimal upper bounds for functional data distributions whose coordinates decay geometrically or polynomially. Moreover, our bounds of the expected value can be combined with mean-concentration results to yield improved exponential tail probability bounds for the Wasserstein error of empirical measures under a Bernstein-type tail condition.},
archivePrefix = {arXiv},
arxivId = {1804.10556},
author = {Lei, Jing},
